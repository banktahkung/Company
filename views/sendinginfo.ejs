<!DOCTYPE html>
<html>
  <head>
    <title>Sending Info</title>
    <link rel="stylesheet" text="text/css" href="/css/sendinginfo.css" />
    <script src="/js/sendinginfo.js"></script>
  </head>

  <body>
    <div
      onclick="SendingSignal(this)"
      class="Infobutton"
      style="background-color: black; color: #fff"
    >
      Sending Black Signal
    </div>
    <div
      onclick="SendingSignal(this)"
      class="Infobutton"
      style="background-color: yellow"
    >
      Sending Yellow Signal
    </div>
    <div
      onclick="SendingSignal(this)"
      class="Infobutton"
      style="background-color: blue; color: #fff"
    >
      Sending Blue Signal
    </div>
    <div
      onclick="SendingSignal(this)"
      class="Infobutton"
      style="background-color: purple; color: #fff"
    >
      Sending Purple Signal
    </div>

    <div
      onclick="PostInfo('create')"
      class="Infobutton"
      style="background-color: aqua; color: #000"
    >
      Create Cart
    </div>
    <div
      onclick="SendingCheck()"
      class="Infobutton"
      style="background-color: bisque; color: #000"
    >
      Sending Check
    </div>
    <div
      onclick="ResetInfo()"
      class="Infobutton"
      style="background-color: magenta; color: #fff"
    >
      Reset Button
    </div>

    <div class="CartIDInput">
      <input
        type="text"
        id="CartIDInput"
        name="CartIDInput"
        style="width: 300px; height: 50px; font-size: 20px; margin-left: 30px;"
      />
    </div>
  </body>
</html>
